
public class MultiThreading_1 {

	public void run() {
		System.out.println("Inside the run()");
	}
	
	public static void main(String[] args) {
		Thread  t = Thread.currentThread();
		System.out.println(t.getName());
		t.setName("Ishmeet's First Thread");
		System.out.println(t.getName());
		

	}

}
